{
  "name": "@sirpepe/shed",
  "version": "3.1.0",
  "sideEffects": false,
  "description": "General-purpose utilities for objects, arrays, maps and types",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/min/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./array": {
      "import": "./dist/esm/array.js",
      "require": "./dist/cjs/array.js"
    },
    "./assert": {
      "import": "./dist/esm/assert.js",
      "require": "./dist/cjs/assert.js"
    },
    "./BiMap": {
      "import": "./dist/esm/BiMap.js",
      "require": "./dist/cjs/BiMap.js"
    },
    "./error": {
      "import": "./dist/esm/error.js",
      "require": "./dist/cjs/error.js"
    },
    "./FRDY": {
      "import": "./dist/esm/FRDY.js",
      "require": "./dist/cjs/FRDY.js"
    },
    "./function": {
      "import": "./dist/esm/function.js",
      "require": "./dist/cjs/function.js"
    },
    "./guard": {
      "import": "./dist/esm/guard.js",
      "require": "./dist/cjs/guard.js"
    },
    "./iterable": {
      "import": "./dist/esm/iterable.js",
      "require": "./dist/cjs/iterable.js"
    },
    "./map": {
      "import": "./dist/esm/map.js",
      "require": "./dist/cjs/map.js"
    },
    "./number": {
      "import": "./dist/esm/number.js",
      "require": "./dist/cjs/number.js"
    },
    "./object": {
      "import": "./dist/esm/object.js",
      "require": "./dist/cjs/object.js"
    },
    "./regexp": {
      "import": "./dist/esm/regexp.js",
      "require": "./dist/cjs/regexp.js"
    },
    "./string": {
      "import": "./dist/esm/string.js",
      "require": "./dist/cjs/string.js"
    },
    "./set": {
      "import": "./dist/esm/set.js",
      "require": "./dist/cjs/set.js"
    },
    "./sortedArray": {
      "import": "./dist/esm/SortedArray.js",
      "require": "./dist/cjs/SortedArray.js"
    },
    "./sorting": {
      "import": "./dist/esm/sorting.js",
      "require": "./dist/cjs/sorting.js"
    },
    "./UnsafeMap": {
      "import": "./dist/esm/UnsafeMap.js",
      "require": "./dist/cjs/UnsafeMap.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/index.d.ts",
        "./dist/types/*"
      ]
    }
  },
  "directories": {
    "test": "test"
  },
  "repository": "git+https://github.com/SirPepe/shed.git",
  "author": "peter@peterkroener.de",
  "bugs": {
    "url": "https://github.com/SirPepe/shed/issues"
  },
  "homepage": "https://github.com/SirPepe/shed#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "docs": "typedoc --exclude src/index.ts --plugin typedoc-plugin-markdown --readme none src/*.ts",
    "lint": "eslint src test",
    "test": "jest",
    "test-dev": "jest --watch",
    "clean": "rm -rf dist && rm -rf docs",
    "build-ts": "tsc -p tsconfig.build.json -d --emitDeclarationOnly --declarationDir dist/types",
    "build-js": "rollup -c",
    "build": "npm run clean && npm run build-ts && npm run build-js && npm run docs",
    "prepareRelease": "rm -rf node_modules && npm install && npm run build && npm run test",
    "release": "release-it"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "prettier": "^2.3.2",
    "release-it": "^15.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-license": "^3.0.0",
    "typedoc": "^0.23.10",
    "typedoc-plugin-markdown": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.14.5",
    "core-js": "^3.14.0"
  }
}
