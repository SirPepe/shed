{
  "name": "@sirpepe/shed",
  "version": "0.2.0",
  "sideEffects": false,
  "description": "General-purpose utilities for objects, arrays, maps and types",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/min/index.js",
  "types": "dist/types/index.d.ts",
  "directories": {
    "test": "test"
  },
  "repository": "git+https://github.com/SirPepe/shed.git",
  "author": "peter@peterkroener.de",
  "bugs": {
    "url": "https://github.com/SirPepe/shed/issues"
  },
  "homepage": "https://github.com/SirPepe/shed#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "docs": "typedoc --plugin typedoc-plugin-markdown --readme none src/*.ts",
    "lint": "eslint src test",
    "test": "jest",
    "test-dev": "jest --watch",
    "clean": "rm -rf dist && rm -rf docs",
    "build-ts": "tsc -p tsconfig.build.json -d --emitDeclarationOnly --declarationDir dist/types",
    "build-js": "rollup -c",
    "build": "npm run clean && npm run build-ts && npm run build-js && npm run docs",
    "prepareRelease": "rm -rf node_modules && npm install && npm run build && npm run test",
    "release": "npm run prepareRelease && npm run release-it"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/jest": "^26.0.22",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "release-it": "^14.9.0",
    "rollup": "^2.44.0",
    "rollup-plugin-license": "^2.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "typedoc": "^0.20.12",
    "typedoc-plugin-markdown": "^3.6.0",
    "typescript": "^4.2.0"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.13.10",
    "core-js": "^3.8.2"
  }
}
